# Appwrite
VITE_APPWRITE_ENDPOINT=https://cloud.appwrite.io/v1
VITE_APPWRITE_PROJECT_ID=你的项目ID
VITE_APPWRITE_DATABASE_ID=你的数据库ID
VITE_APPWRITE_TRACKS_COLLECTION_ID=你的tracks集合ID
VITE_APPWRITE_TASKS_COLLECTION_ID=你的tasks集合ID
VITE_APPWRITE_UPLOADS_BUCKET_ID=你的uploads桶ID
VITE_APPWRITE_STEMS_BUCKET_ID=你的stems桶ID
VITE_APPWRITE_GENERATED_BUCKET_ID=你的generated桶ID

# Suno API — 只填 Cloudflare Worker URL，不需要也不应该填 API Key
# API Key 通过 wrangler secret put SUNO_API_KEY 存储在 Worker 环境变量里
VITE_SUNO_WORKER_URL=https://你的worker子域名.workers.dev

# Demucs API (5090 服务器)
VITE_DEMUCS_API_BASE_URL=http://你的5090服务器IP:8000
